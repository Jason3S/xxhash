{
  "name": "js-xxhash",
  "version": "1.0.4",
  "description": "Pure JS implementation of xxhash",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**",
    "!**/*.map",
    "!**/*.test.*"
  ],
  "scripts": {
    "clean": "rimraf dist/**",
    "test": "mocha --recursive \"./dist/**/*.test.js\"",
    "test-watch": "mocha --watch --require ts-node/register --recursive \"src/**/*.test.ts\"",
    "build": "tsc",
    "clean-build": "npm run clean ; npm run build",
    "coverage": "NODE_ENV=test nyc npm run test-ts",
    "lint": "eslint .",
    "test-ts": "NODE_ENV=test mocha --require ts-node/register --recursive \"src/**/*.test.ts\"",
    "coverage-coveralls": "nyc report --reporter=text-lcov | coveralls",
    "travis-coverage": "npm run coverage && npm run coverage-coveralls",
    "prepare": "npm run build",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jason3S/xxhash.git"
  },
  "keywords": [
    "xxhash",
    "javascript",
    "typescript"
  ],
  "author": "Jason Dent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jason3S/xxhash/issues"
  },
  "homepage": "https://github.com/Jason3S/xxhash#readme",
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/lorem-ipsum": "^2.0.0",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.2",
    "@types/xxhashjs": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "chai": "^4.3.4",
    "coveralls": "^3.1.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "lorem-ipsum": "^2.0.3",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "prettier": "^2.3.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4",
    "xxhashjs": "^0.2.2"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.test.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
